{
  "name": "com.reuters-graphics.ai2svelte",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "watch": "tsc && vite build --watch true",
    "build": "rimraf dist/* && tsc -p \"tsconfig-build.json\" && vite build --watch false",
    "zxp": "rimraf dist/* && tsc -p \"tsconfig-build.json\" && cross-env ZXP_PACKAGE=true vite build --watch false",
    "zip": "rimraf dist/* && tsc -p \"tsconfig-build.json\" && cross-env ZIP_PACKAGE=true vite build --watch false",
    "serve": "cross-env SERVE_PANEL=true vite preview",
    "symlink": "cross-env BOLT_ACTION=symlink vite",
    "delsymlink": "cross-env BOLT_ACTION=delsymlink vite",
    "bake-shadows-raw": "node scripts/bakeShadows.js",
    "bake-shadows": "npm run bake-shadows-raw -- --input=src/js/main/Tabs/data/shadows.json --output=src/js/main/Tabs/data/shadowsBaked.json"
  },
  "dependencies": {
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/legacy-modes": "^6.5.1",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@floating-ui/dom": "^1.7.2",
    "@fsegurai/codemirror-theme-bundle": "^6.2.0",
    "@reuters-graphics/vite-plugin-purge-styles": "^0.0.3",
    "codemirror": "^6.0.2",
    "codemirror-theme-catppuccin": "^0.3.0",
    "json5": "^2.2.3",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "prism-code-editor": "^4.1.0",
    "underscore": "^1.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.7",
    "@types/trusted-types": "^2.0.2",
    "babel-plugin-transform-scss": "^1.2.0",
    "babel-preset-env": "^1.7.0",
    "core-js": "3",
    "cross-env": "^7.0.3",
    "es-check": "^9.1.4",
    "eslint": "^9.30.1",
    "extendscriptr": "^1.2.5",
    "fs-extra": "^10.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^4.42.0",
    "rollup-plugin-multi-input": "^1.5.0",
    "rollup-plugin-node-copy": "^1.0.4",
    "rollup-plugin-scss": "^4.0.1",
    "sass": "^1.89.1",
    "source-map": "^0.7.4",
    "svelte": "^5.33.14",
    "svelte-awesome-color-picker": "^4.0.2",
    "svelte-check": "^4.2.1",
    "svelte-preprocess": "^6.0.3",
    "svooltip": "^0.9.0",
    "types-for-adobe": "^7.2.3",
    "types-for-adobe-extras": "^0.0.9",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^6.3.5",
    "vite-cep-plugin": "^2.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "esbuild",
      "jsxbin",
      "svelte-preprocess"
    ]
  }
}
